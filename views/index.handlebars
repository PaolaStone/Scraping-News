{{#each articles}}
<div class = "row">
    <div class="card" id={{this.id}}>
        <h5 class="card-header">{{this.title}}</h5>
        <div class="card-body">
            <p class="card-text">{{this.summary}}</p>
            <a href="{{this.link}}" class=">{{this.link}}</a>
            <a href="#" class="btn btn-secondary">Save</a>
            <a href="#" class="btn btn-secondary">Add Note</a>
        </div>
    </div>
</div>
{{/each}}

